generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  name String
  email String
}

model Sell{
  id Int @id @default(autoincrement()) 
  sellname String
  price Int
}

model Buy{
  id Int @id @default(autoincrement()) 
  buy String
  Price Int 
}